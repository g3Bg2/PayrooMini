{
  "name": "api",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "tsx watch --tsconfig ./tsconfig.json src/index.ts",
    "build": "tsc -p tsconfig.json",
    "start": "node dist/index.js",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:unit": "vitest run --dir src/domain",
    "prisma:generate": "prisma generate",
    "seed": "ts-node prisma/seed.ts",
    "lint": "eslint . --ext .ts,.tsx",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@fastify/cors": "^8.0.0",
    "@prisma/client": "^6.15.0",
    "bcrypt": "^6.0.0",
    "fastify": "^4.0.0",
    "jsonwebtoken": "^9.0.2",
    "luxon": "^3.0.0",
    "pino": "^8.0.0",
    "zod": "^3.0.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^6.0.0",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/luxon": "^3.7.1",
    "@types/node": "^24.3.0",
    "@vitest/coverage-v8": "^1.0.0",
    "jest": "^29.7.0",
    "pino-pretty": "^13.1.1",
    "prisma": "^6.15.0",
    "supertest": "^6.0.0",
    "ts-jest": "^29.4.1",
    "ts-node": "^10.0.0",
    "ts-node-dev": "^2.0.0",
    "tsx": "^4.20.5",
    "typescript": "^5.0.0",
    "vitest": "^1.0.0",
    "vitest-mock-extended": "^3.1.0"
  }
